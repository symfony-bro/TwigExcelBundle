{
    "name": "symfony-bro/twig-excel-bundle",
    "type": "symfony-bundle",
    "description": "This Symfony bundle provides a PhpExcel integration for Twig (fork)",
    "keywords": ["excel","phpexcel","twig","symfony"],
    "homepage": "https://github.com/MewesK/TwigExcelBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mewes Kochheim",
            "email": "mewes@kochheim.de"
        },
        {
            "name": "Community Contributors",
            "homepage": "https://github.com/MewesK/TwigExcelBundle/graphs/contributors"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/framework-bundle": "^3.3",
        "phpoffice/phpexcel": "~1.8.1"
    },
    "require-dev": {
        "symfony/symfony": "^3.3",
        "symfony/phpunit-bridge": "^3.3",
        "sensio/framework-extra-bundle": "^3.3",
        "mpdf/mpdf": "~6.0"
    },
    "minimum-stability": "dev",
    "suggest": {
        "mpdf/mpdf": "Add support for PDF rendering, requires ~6.0"
    },
    "autoload": {
        "psr-4": { "MewesK\\TwigExcelBundle\\": "" }
    }
}
